{
    "name": "browser-extension-v3",
    "version": "0.0.1",
    "private": true,
    "description": "AdGuard browser extension",
    "author": "adguard@adguard.com",
    "license": "LGPL-3.0",
    "scripts": {
        "dev": "BUILD_ENV=dev run-p build:*",
        "beta": "BUILD_ENV=beta run-p build:*",
        "release": "BUILD_ENV=release run-p build:*",
        "build:chrome": "BROWSER=chrome webpack -c tasks/webpack.config.ts",
        "build:edge": "BROWSER=edge webpack -c tasks/webpack.config.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "watch": "BUILD_ENV=dev yarn build:chrome --watch",
        "ci:increment": "yarn version --patch --no-git-tag-version",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
