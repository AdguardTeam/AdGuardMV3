.icon {
    display: inline-block;
    vertical-align: middle;
}

.iconButton {
    composes: icon;
    width: 2.4rem;
    height: 2.4rem;
    flex-shrink: 0;
}

.iconLogo {
    composes: icon;
    width: 11.3rem;
    height: 2.5rem;
}

.iconAction {
    composes: icon;
